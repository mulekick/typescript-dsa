{
    "name": "story-structure",
    "version": "1.0.0",
    "private": true,
    "description": "Data structures & algorithms implementations with jest unit tests for benchmarking.",
    "type": "module",
    "scripts": {
        "list": "npx tsc --project tsconfig.json --listFilesOnly | sort",
        "typecheck": "npx tsc --project tsconfig.json --noEmit --emitDeclarationOnly false && echo 'type checking was successful'",
        "lint": "npx eslint .",        
        "test": "npx jest --testPathPatterns 'src/tests/.*.test.ts'",
        "asyncqueue": "npx tsx src/asyncQueue.ts",
        "docs:build": "npx typedoc"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-typescript": "^7.27.1",
        "@jest/globals": "^30.0.0",
        "@mulekick/eslint-config-muleslint": "^2.2.2",
        "@types/node": "^24.0.1",
        "babel-plugin-transform-import-meta": "^2.3.3",
        "core-js": "^3.43.0",
        "eslint": "^9.29.0",
        "globals": "^16.2.0",
        "jest": "^30.0.0",
        "tsx": "^4.20.3",
        "typedoc": "^0.28.5",
        "typedoc-plugin-markdown": "^4.6.4",
        "typescript": "^5.8.3"
    },
    "engines": {
        "node": ">=v22.15.0",
        "npm": ">=11.3.0"
    },
    "os": [
        "linux"
    ],
    "author": "mulekick <moolekick@gmail.com> (https://github.com/mulekick)",
    "license": "MIT",
    "repository": {},
    "bugs": {},
    "homepage": "https://github.com/mulekick/story-structure"
}
